{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-4-data-engineering-rk"
		},
		"azure_data_lake_4_data_engineering_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azure_data_lake_4_data_engineering'"
		},
		"azure_db_4_data_engineering_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'azure_db_4_data_engineering'"
		},
		"azure_data_lake_4_data_engineering_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sa4dataengineering4rk.dfs.core.windows.net/"
		},
		"azure_db_4_data_engineering_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-server-4-data-engineering.database.windows.net"
		},
		"azure_db_4_data_engineering_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-4-data-engineering"
		},
		"azure_db_4_data_engineering_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "real_mfalme"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-4-data-engineering-rk_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azure_data_lake_4_data_engineering_accountKey": {
						"value": "[parameters('azure_data_lake_4_data_engineering_accountKey')]"
					},
					"azure_db_4_data_engineering_password": {
						"value": "[parameters('azure_db_4_data_engineering_password')]"
					},
					"azure_data_lake_4_data_engineering_properties_typeProperties_url": {
						"value": "[parameters('azure_data_lake_4_data_engineering_properties_typeProperties_url')]"
					},
					"azure_db_4_data_engineering_properties_typeProperties_server": {
						"value": "[parameters('azure_db_4_data_engineering_properties_typeProperties_server')]"
					},
					"azure_db_4_data_engineering_properties_typeProperties_database": {
						"value": "[parameters('azure_db_4_data_engineering_properties_typeProperties_database')]"
					},
					"azure_db_4_data_engineering_properties_typeProperties_userName": {
						"value": "[parameters('azure_db_4_data_engineering_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}